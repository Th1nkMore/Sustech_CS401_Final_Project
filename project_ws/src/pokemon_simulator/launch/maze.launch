<launch>     
    <!-- 设置launch文件的参数 -->
    <arg name="world_name" value="$(find pokemon_simulator)/worlds/pokemon_maze_cs405.world"/>

    <arg name="x_pos" default="2.0"/>
    <arg name="y_pos" default="-2.7"/>
    <arg name="z_pos" default="0"/>  
    <arg name="yaw"   default=" 1.557316"/>
  <!-- 运行gazebo仿真环境 -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(arg world_name)" />
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>  
  <param name="robot_description" command="$(find xacro)/xacro $(find mycar_description)/urdf/turtlebot3.kinect.xacro" />
  <!-- <param name="robot_description" command="$(find xacro)/xacro $(find mycar_description)/urdf/turtlebot3.urdf.xacro" /> -->
  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3_burger -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -Y $(arg yaw) -param robot_description" />

  <!-- <node pkg="depthimage_to_laserscan" type="depthimage_to_laserscan" name="depthimage_to_laserscan" output="screen">
    <remap from="image" to="/kinect/depth/image_raw" />
    <remap from="camera_info" to="/kinect/depth/camera_info" />
    <remap from="scan" to="/scan" />
    <param name="output_frame_id" value="/kinect_link" />
  </node> -->
</launch>
